<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".coordinator_dashboard">

    <EditText
        android:id="@+id/range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,UnknownId"
        tools:layout_editor_absoluteY="32dp" />

    <Button
        android:id="@+id/rangecall"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toEndOf="parent"
        android:layout_below="@id/range"
        android:layout_height="wrap_content"
        android:text="Call for next" />

    <TextView
        android:id="@+id/rangedesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rangecall"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="80dp" />

    <ListView
        android:id="@+id/colist"
        android:layout_width="409dp"
        android:layout_height="630dp"
        android:layout_below="@id/rangedesc"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="100dp">

    </ListView>
</RelativeLayout>